                    "target": {
                        "selector": ".ot-desc-cntr, .description-container"
                    },
                    "action": {
                        "type": "list",
                        "actions": [
                            {
                                "type": "ifcss",
                                "target": {
                                    "selector": ".ot-grp-hdr1, .group-toggle",

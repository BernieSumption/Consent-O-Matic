{
    "$schema": "https://raw.githubusercontent.com/cavi-au/Consent-O-Matic/master/rules.schema.json",
    "onetrust_pctab": {
        "detectors": [
            {
                "presentMatcher": [
                    {
                        "type": "css",
                        "target": {
                            "childFilter": {
                                "target": {
                                    "selector": ".otPcTab"
                                }
                            },
                            "selector": "#onetrust-consent-sdk"
                        }
                    }
                ],
                "showingMatcher": [
                    {
                        "type": "css",
                        "target": {
                            "childFilter": {
                                "target": {
                                    "selector": ".otPcTab",
                                    "displayFilter": true
                                }
                            },
                            "selector": "#onetrust-consent-sdk"
                        }
                    }
                ]
            }
        ],
        "methods": [
            {
                "name": "OPEN_OPTIONS"
            },
            {
                "action": {

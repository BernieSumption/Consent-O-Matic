                                    ]
                                },
                                "trueAction": {
                                    "type": "consent",
                                    "consents": [
                                        {
                                            "matcher": {
                                                "type": "checkbox",
                                                "target": {
                                                    "selector": ".ot-grp-hdr1 input, .group-toggle input"
                                                }
                                            },
                                            "toggleAction": {
                                                "type": "click",
                                                "target": {
                                                    "selector": ".ot-grp-hdr1 input, .group-toggle input"
                                                }
                                            },
                                            "type": "D"
